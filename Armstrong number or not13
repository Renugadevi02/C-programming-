#include <stdio.h>
int main() {
    int n, original, remainder, result = 0;
    scanf("%d", &n);
    if (n >= 100 && n <= 999) {
        original = n;
        while (original != 0) {
            remainder = original % 10;
            result += remainder * remainder * remainder;
            original /= 10;
        }
        if (result == n)
            printf("Yes\n");
        else
            printf("No\n");
    } else {
        printf("No\n");
    }

    return 0;
}
